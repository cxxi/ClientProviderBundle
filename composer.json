{
    "name": "cxxi/client-provider-bundle",
    "type": "symfony-bundle",
    "version": "dev-main",
    "description": "Client provider abstraction integration for Symfony",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/config": "^6.2",
        "symfony/dependency-injection": "^6.2",
        "symfony/http-kernel": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Cxxi\\ClientProviderBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cxxi\\ClientProviderBundle\\Tests\\": "tests/"
        },
        "files": [
            "tests/Mock/SampleClassWithAttribute.php",
            "tests/Mock/SampleClassWithValidAttribute.php",
            "tests/Mock/SampleClassWithInvalidAttribute.php",
            "tests/Mock/SampleValidAttribute.php",
            "tests/Mock/SampleInvalidAttribute.php",
            "tests/Mock/SampleNotFoundAttribute.php"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.2.*"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4",
        "symfony/maker-bundle": "^1.61"
    }
}
